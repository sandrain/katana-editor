/* CUSTOM GROUPBOX STYLE */

QGroupBox#groupbox::title {
subcontrol-origin: border;
subcontrol-position: top left;
position: relative;
margin-left: 2px;
margin-right: 2px;
top: -7px;
left: 5px;
}
QGroupBox#groupbox {
font-family: Arial;
font-weight: bold;
color: rgb(255,255,255);
border-width: 3px;
border-style: solid;
border-radius: 5px;
margin-top: 8px;
padding: 2px;
background-color: rgba(255,255,255,50);
}
 
