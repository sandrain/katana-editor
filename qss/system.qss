/* CUSTOM QTREEWIDGET STYLE */
bankTreeList#banklist{
}
QTreeWidget#banklist {
font-family: Arial;
font-weight: bold;
border-style: solid;
border-radius: 10px;
border-width: 1px;
}
QTreeWidget#banklist QHeaderView::section:only-one {
font-family: Arial;
font-weight: bold;
border-style: solid;
border-radius: 3px;
border-width: 1px;
}
QTreeWidget#banklist::branch {
margin: 0px;
width: 9px;
}

/* CUSTOM DISPLAY STYLE */

#display {
